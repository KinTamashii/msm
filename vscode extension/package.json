{
  "name": "msm",
  "displayName": "MSM",
  "description": "Message Studio Markup Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "languages": [{
      "id": "msm",
      "aliases": ["Message Studio Markup", "msm"],
      "extensions": [".msm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "msm",
      "scopeName": "source.msm",
      "path": "./syntaxes/msm.tmLanguage.json"
    }],
    "properties": {
      "msm.executablePath": {
        "type": "string",
        "default": "/Users/kin_tamashii/Desktop/c++/projects/in-progress/msbt-editor++/build/msbt-editor++",
        "description": "Path to encode the msm files."
      },
      "msm.encodeArgs": {
        "type": "string",
        "default": "",
        "description": "Parameters passed to the executable to encode."
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
